{
    "name": "hanoivip/ddd2",
    "description": "DDD2 Auth",
    "type": "library",
    "license": "MIT",
    "autoload" : {
		"psr-4" : {
			"Hanoivip\\Ddd2\\" : "src/"
		}
	},
	"repositories" : [{
			"type" : "path",
			"url" : "../events",
			"options" : {
				"symlink" : true
			}
		}, {
                        "type" : "path",
                        "url" : "../user",
                        "options" : {
                                "symlink" : true
                        }
                }
	],
	"require" : {
		"php" : ">=7.0.0",
		"laravel/framework" : ">=5.5",
		"mervick/curl-helper" : "^2.2",
		"mews/captcha": "^3.0",
		"hanoivip/events" : "dev-master",
		"hanoivip/user": "dev-master"
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Hanoivip\\Ddd2\\AuthServiceProvider",
				"Hanoivip\\Ddd2\\EventServiceProvider"
			]
		}
	}
}
